{
  "name": "front-end",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "proxy": "http://localhost:4000",
  "dependencies": {
    "@dfuse/client": "^0.3.10",
    "@material-ui/core": "^4.9.9",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.20.0",
    "eosjs": "^20.0.3",
    "eosjs-ecc": "^4.0.7",
    "moment": "^2.24.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "typeface-roboto": "0.0.75"
  },
  "scripts": {
    "start": "react-scripts start",
    "docker": "REACT_APP_NODE_ENV=docker react-scripts start",
